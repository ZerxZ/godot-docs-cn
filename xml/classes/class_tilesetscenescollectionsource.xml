                    <literal_block force="False" highlight_args="{}" language="gdscript" linenos="False" xml:space="preserve">var source_id = tile_map_layer.get_cell_source_id(Vector2i(x, y))
if source_id &gt; -1:
    var scene_source = tile_map_layer.tile_set.get_source(source_id)
    if scene_source is TileSetScenesCollectionSource:
        var alt_id = tile_map_layer.get_cell_alternative_tile(Vector2i(x, y))
        # The assigned PackedScene.
        var scene = scene_source.get_scene_tile_scene(alt_id)</literal_block>
