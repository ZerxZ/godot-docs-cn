public override void _Process(double delta)
{
    long taskId = WorkerThreadPool.AddGroupTask(Callable.From&lt;int&gt;(ProcessEnemyAI), _enemies.Count);
    // 其他代码……
    WorkerThreadPool.WaitForGroupTaskCompletion(taskId);
