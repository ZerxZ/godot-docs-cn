            <target refid="class-cameraattributesphysical-property-auto-exposure-max-exposure-value"></target>
            <paragraph classes="classref-property" ids="class-cameraattributesphysical-property-auto-exposure-max-exposure-value" names="class_cameraattributesphysical_property_auto_exposure_max_exposure_value"><reference internal="True" refuri="class_float#class-float"><inline classes="std std-ref">float</inline></reference> <strong>auto_exposure_max_exposure_value</strong> = <literal>10.0</literal></paragraph>
            <bullet_list bullet="-" classes="classref-property-setget">
                <list_item>
                    <paragraph>void <strong>set_auto_exposure_max_exposure_value</strong> <strong>(</strong> <reference internal="True" refuri="class_float#class-float"><inline classes="std std-ref">float</inline></reference> value <strong>)</strong></paragraph>
