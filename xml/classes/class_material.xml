            <bullet_list bullet="-" classes="classref-property-setget">
                <list_item>
                    <paragraph><abbreviation explanation="No return value.">void</abbreviation> <strong>set_next_pass</strong>(value: <reference internal="True" refid="class-material"><inline classes="std std-ref">Material</inline></reference>)</paragraph>
