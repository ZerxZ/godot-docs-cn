                    <literal_block force="False" highlight_args="{}" language="gdscript" linenos="False" xml:space="preserve">@export var curve: Curve

func _ready():
    var tween = create_tween()
    # Interpolate the value using a custom curve.
    tween.tween_property(self, "position:x", 300, 1).as_relative().set_custom_interpolator(tween_curve)

func tween_curve(v):
    return curve.sample_baked(v)</literal_block>
